{
  "Proxy": {
    "Sites": [
      {
        "Source": "localhost",
        "Target": "https://dl.flathub.org",
        "Settings": [
          {
            "Match": "\\.filez$",
            "LifeTimeSeconds": 63115200
          },
          {
            "Match": "\\.local$",
            "LifeTimeSeconds": 63115200
          }
        ]
      },
      {
        "Source": "flathub.markridgwell.com",
        "Target": "https://dl.flathub.org",
        "Settings": [
          {
            "Match": "\\.filez$",
            "LifeTimeSeconds": 63115200
          },
          {
            "Match": "\\.local$",
            "LifeTimeSeconds": 63115200
          },
          {
            "Match": "^/repo/deltas/",
            "LifeTimeSeconds": 63115200
          }
        ]
      },
      {
        "Source": "pacman.markridgwell.com",
        "Target": "http://pacman.local:8888",
        "Settings": [
          {
            "Match": "\\.pkg\\.tar\\.zst\\.sig$",
            "LifeTimeSeconds": 63115200
          },
          {
            "Match": "\\.pkg\\.tar\\.zst$",
            "LifeTimeSeconds": 63115200
          }
        ]
      }
    ],
    "Storage": "/data"
  }
}